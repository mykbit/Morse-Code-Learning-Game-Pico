{
  "cmake.statusbar.advanced": {
    "debug": {
      "visibility": "hidden"
    },
    "launch": {
      "visibility": "default"
    },
    "launchTarget": {
      "visibility": "default"
    },
    "build": {
      "visibility": "default"
    },
    "buildTarget": {
      "visibility": "default"
    }
  },
  "cmake.configureEnvironment": {
    "PROJECT_NAME": "morse_code"
